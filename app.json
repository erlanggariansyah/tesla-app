{
  "expo": {
    "name": "Tesla",
    "slug": "Tesla",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "native",
      "backgroundColor": "#ffffff",
      "width": 155,
      "height": 20
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.erlanggariansyah.Tesla"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
